{
    "rules": {
        "indent": [ 2, 4, {
          "SwitchCase": 1
        } ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always"],
        "no-use-before-define": [2, "nofunc"],
        "strict": [2, "global"],
        "no-var": [ 0 ],
        "comma-dangle": [ 0 ],
        "func-names": [ 0 ],
        "object-shorthand": [ 0 ],
        "no-trailing-spaces": [ 0 ],
        "padded-blocks": [ 0 ],
        "no-param-reassign": [2, { "props": false }],
        "no-console": 0,
        "no-new-func": 0
    },
    "extends": "airbnb",
    "plugins": [
        "react"
    ]
}